{
    "name": "vufind/vufind",
    "description": "A flexible discovery layer.",
    "authors": [
        {
            "name": "Demian Katz",
            "email": "demian.katz@villanova.edu"
        }
    ],
    "license": "GPL-2.0",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        },
        {
