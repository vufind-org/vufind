        "pear-pear.php.net/pear": "*",
        "pear-pear.php.net/validate_ispn": "*",
        "serialssolutions/summon": "*",
        "symfony/yaml": "*",
        "vufind-org/vufindcode": "*",
        "vufind-org/vufindhttp": "*",
